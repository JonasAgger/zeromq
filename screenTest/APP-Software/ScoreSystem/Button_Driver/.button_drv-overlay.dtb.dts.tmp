/dts-v1/;
/plugin/;

/ {
  compatible = "brcm,bcm2835", "brcm,bcm2836", "brcm,bcm2708", "brcm,bcm2709";

  fragment@0 {

    target-path = "/";
    __overlay__ {

      knap: knap@0 {

        compatible = "ase, knap";


        gpios = <&gpio 10 0>, <&gpio 9 0>, <&gpio 11 0>;

        status = "okay";
      };
    };
  };
};
